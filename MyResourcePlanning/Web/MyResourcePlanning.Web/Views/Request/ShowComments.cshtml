@model List<string>

@{
    ViewData["Title"] = "Show Comments";
}

    <div class="jumbotron">
        <h1 class="text-center" mt-3>Comments</h1>
        <a class="btn btn-secondary" href="javascript:void(0);" onclick="history.go(-1);">Back to requests</a>
        <hr class="mt-3 bg-resources hr-2" />
        @foreach (var item in Model)
        {
            <textarea readonly class="form-control mb-3" rows="3" id="comment">@item</textarea>
        }
    </div>


